{
  "name": "nextjs",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.7",
    "@types/react": "^16.9.51",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "semantic-release": "^17.1.2",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm shrinkwrap && git add npm-shrinkwrap.json",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Frannca/nextjs.git"
  }
}
